      <memberdef kind="function" id="classgcn_1_1Icon_1a1533b6c01b209ff077b9fc37af364db7" prot="public" static="no" const="no" explicit="no" inline="no" virt="virtual">
        <type><ref refid="shaders_8cpp_1a7206f6ae7a09ba9ca63f80e09015869c" kindref="member">void</ref></type>
